@{
  Layout = "_Layout";
}

@using HairSalon.Models


<div class="row">
  <div class="col-md-9">
    @if(Model.Count == 0)
    {
      <div class="panel panel-primary">
        <div class="panel-heading">
          <div class="panel-title">
            <strong>No Results</strong>
          </div>
        </div>
        <div class="panel-body">
          <p>You do not have any stylists at this time.</p>
        </div>
      </div>
    }
  </div>
</div>